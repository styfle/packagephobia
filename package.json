{
  "name": "packagephobia",
  "version": "1.0.0",
  "description": "Find the cost of adding a new dev dependency to your project",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "test": "tape dist/test/**/*.js",
    "deploy": "now --docker --public && now alias"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/styfle/packagephobia.git"
  },
  "keywords": [],
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "npm": "^6.4.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "redis": "^2.8.0",
    "semver": "^5.5.1"
  },
  "devDependencies": {
    "@types/node": "^10.11.3",
    "@types/node-fetch": "^2.1.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/redis": "^2.8.6",
    "@types/semver": "^5.5.0",
    "@types/tape": "^4.2.32",
    "prettier": "^1.14.3",
    "tape": "^4.9.1",
    "typescript": "^3.1.1"
  }
}

{
  "name": "packagephobia",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "git-pre-commit": "npm run prettier && git add -A",
    "lint": "npm run prettier && git diff --exit-code",
    "test": "tsc && rm -f test/hardlink/link && ln test/hardlink/orig.txt test/hardlink/link && tape \"dist/test/**/*.js\""
  },
  "repository": "styfle/packagephobia",
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "badgen": "^3.2.2",
    "node-fetch": "^2.6.1",
    "npm": "^7.20.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redis": "^3.1.2",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/redis": "^2.8.31",
    "@types/semver": "^7.3.8",
    "@types/tape": "^4.13.1",
    "@vercel/git-hooks": "^1.0.0",
    "prettier": "^2.3.2",
    "tape": "^5.2.2",
    "typescript": "^4.3.5"
  }
}

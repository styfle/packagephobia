{
  "name": "packagephobia",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "git-pre-commit": "npm run prettier && git add -A",
    "lint": "npm run prettier && git diff --exit-code",
    "test": "tsc && rm -f test/hardlink/link && ln test/hardlink/orig.txt test/hardlink/link && node --test ./dist/test/**/*.test.js"
  },
  "repository": "styfle/packagephobia",
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "badgen": "^3.2.3",
    "ioredis": "^5.9.2",
    "lru-cache": "^11.2.4",
    "npm": "^11.8.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@types/node": "^22.19.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/semver": "^7.5.8",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "npm@11.8.0"
}

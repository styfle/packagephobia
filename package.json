{
  "name": "packagephobia",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "git-pre-commit": "npm run prettier && git add -A",
    "lint": "npm run prettier && git diff --exit-code",
    "test": "tsc && rm -f test/hardlink/link && ln test/hardlink/orig.txt test/hardlink/link && tape \"dist/test/**/*.js\""
  },
  "repository": "styfle/packagephobia",
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "badgen": "^3.2.2",
    "ioredis": "^5.0.5",
    "npm": "^6.14.17",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/node": "^16.11.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/semver": "^7.3.9",
    "@types/tape": "^4.13.2",
    "@vercel/git-hooks": "^1.0.0",
    "prettier": "^2.6.2",
    "tape": "^5.5.3",
    "typescript": "^4.6.4"
  }
}

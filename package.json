{
  "name": "packagephobia",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "cp -r static public",
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "git-pre-commit": "npm run prettier && git add -A",
    "lint": "npm run prettier && git diff --exit-code",
    "test": "tsc && tape \"dist/test/**/*.js\""
  },
  "repository": "styfle/packagephobia",
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.0",
    "npm": "^6.13.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redis": "^2.8.0",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.1",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/redis": "^2.8.14",
    "@types/semver": "^6.2.0",
    "@types/tape": "^4.2.33",
    "@zeit/git-hooks": "^0.1.4",
    "prettier": "^1.19.1",
    "tape": "^4.12.0",
    "typescript": "^3.7.4"
  }
}

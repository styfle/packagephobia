{
  "name": "packagephobia",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "prettier": "prettier \"./**/*.ts\" \"./**/*.tsx\" --write",
    "git-pre-commit": "npm run prettier && git add -A",
    "lint": "npm run prettier && git diff --exit-code",
    "test": "tsc && rm -f test/hardlink/link && ln test/hardlink/orig.txt test/hardlink/link && tape \"dist/test/**/*.js\""
  },
  "repository": "styfle/packagephobia",
  "author": "styfle",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.1",
    "npm": "^6.14.8",
    "react": "^16.14.0",
    "react-dom": "^16.13.0",
    "redis": "^3.0.2",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/redis": "^2.8.28",
    "@types/semver": "^7.3.4",
    "@types/tape": "^4.13.0",
    "@zeit/git-hooks": "^0.1.4",
    "prettier": "^2.1.2",
    "tape": "^5.0.1",
    "typescript": "^4.0.3"
  }
}
